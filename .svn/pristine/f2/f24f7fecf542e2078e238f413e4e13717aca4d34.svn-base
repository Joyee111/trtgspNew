package com.sinosoft.drugState.outcheck.model;
public class TrtssSalesFormInfoVO {
	private String contractNo;// 合同号即票据号
	private String salesFromNumber;// 销售单号
	private String salesItemNumber;// 销售细单号
	private String commonName;// 通用名称
	private String salesDate;// 销售日期
	private String departmentId;// 部门ID

	public TrtssSalesFormInfoVO() {

	}

	public TrtssSalesFormInfoVO(String contractNo, String salesFromNumber,
			String salesItemNumber, String commonName, String salesDate,
			String departmentId) {
		this.contractNo = contractNo;
		this.salesFromNumber = salesFromNumber;
		this.salesItemNumber = salesItemNumber;
		this.commonName = commonName;
		this.salesDate = salesDate;
		this.departmentId = departmentId;
	}

	public String getContractNo() {
		return contractNo;
	}

	public void setContractNo(String contractNo) {
		this.contractNo = contractNo;
	}

	public String getSalesFromNumber() {
		return salesFromNumber;
	}

	public void setSalesFromNumber(String salesFromNumber) {
		this.salesFromNumber = salesFromNumber;
	}

	public String getSalesItemNumber() {
		return salesItemNumber;
	}

	public void setSalesItemNumber(String salesItemNumber) {
		this.salesItemNumber = salesItemNumber;
	}

	public String getCommonName() {
		return commonName;
	}

	public void setCommonName(String commonName) {
		this.commonName = commonName;
	}

	public String getSalesDate() {
		return salesDate;
	}

	public void setSalesDate(String salesDate) {
		this.salesDate = salesDate;
	}

	public String getDepartmentId() {
		return departmentId;
	}

	public void setDepartmentId(String departmentId) {
		this.departmentId = departmentId;
	}

}
