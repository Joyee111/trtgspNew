package com.sinosoft.qualityRecords.infoFeedback.model;


public class InfoFeedBackItemAndUserVO {
	private Long infoFeedId;
	private String userId;
	private String type;
	private String content;
	private String userName;
	
	public InfoFeedBackItemAndUserVO(){
		
	}
	public InfoFeedBackItemAndUserVO(InfoFeedbackItem item ,String  userName){
		this.infoFeedId = item.getInfoFeedbackId();
		this.type = item.getType();
		this.content = item.getContent();
		this.userName = userName;
	}
	
	public Long getInfoFeedId() {
		return infoFeedId;
	}
	public void setInfoFeedId(Long infoFeedId) {
		this.infoFeedId = infoFeedId;
	}
	public String getUserId() {
		return userId;
	}
	public void setUserId(String userId) {
		this.userId = userId;
	}
	public String getType() {
		return type;
	}
	public void setType(String type) {
		this.type = type;
	}
	public String getContent() {
		return content;
	}
	public void setContent(String content) {
		this.content = content;
	}
	public String getUserName() {
		return userName;
	}
	public void setUserName(String userName) {
		this.userName = userName;
	}
	
}
