#staffInfo .baseInfo { overflow:hidden; zoom:1; padding:0px 13px 15px 13px; }

#staffInfo .baseInfo .biao { height:31px; background:#ffffff; border:1px solid #edd1b1;}

#staffInfo .baseInfo .clear { clear:both;}
#staffInfo .baseInfo p { height:30px; padding-top:32px;text-align: center; float: left; padding-left: 245px;}
#staffInfo .baseInfo p #update { font-family:"宋体"; font-size:13px; color:#aa541a; border:none; height:30px; width:92px; background:url(../images/updateBefore.gif) no-repeat right top; cursor:pointer;}
#staffInfo .baseInfo p #update.current { background:url(../images/updateAfter.gif) no-repeat right top;}
#staffInfo .expendInfo { overflow:hidden; padding:11px 13px 0px 13px; min-width:648px;}




/*2013-3-21*/
#staffInfo .baseInfo .biao h3 { height:29px; border:1px solid #ffffff; background:url(../images/baseInfo_h3.gif) repeat-x left top; line-height:28px; font-family:"宋体"; font-size:14px; color:#a87122; padding-left:13px;}  
#staffInfo .baseInfo .biao h3 img { float:left; margin-top:2px; margin-right:15px; _display:inline;}

.ul_1 { list-style-type:none; float:left; width:304px; margin:27px 36px 0px 0px; border:1px solid #e1be9f; border-bottom:none; _display:inline; overflow:hidden; zoom:1;}
.ul_1 li { float:left; height:26px; line-height:26px; border-bottom:1px solid #e1be9f; font-family:"宋体"; font-size:13px; color:#aa541a;}
.li_title { padding-left:9px; width:95px; border-right:1px solid #e1be9f;}
.li_val { padding-left:10px; width:189px; font-weight:bold;}

#staffInfo .expendInfo .biao { height:31px; padding-right:14px; background:url(../images/expendInfo_biao.gif) repeat-x left top; border:1px solid #edd1b1; }
#staffInfo .expendInfo .biao h3 { float:left; height:31px; line-height:30px; padding-right:19px; font-family:"宋体"; font-size:14px; color:#aa541a; padding-left:10px;}

.cloBtn { float:right; display:block; width:20px; height:19px; margin-top:6px; background:url(../images/cloBtn.gif) no-repeat left top; cursor:pointer; line-height:0; font-size:0;overflow:hidden;}
.shensuoBtn {float:right; display:block; margin-right:6px; width:20px; height:19px; margin-top:6px; background:url(../images/shensuo.gif) no-repeat left top; cursor:pointer; line-height:0; font-size:0;overflow:hidden;}
.shensuo1Btn {float:right; display:block; margin-right:6px; width:20px; height:19px; margin-top:6px; background:url(../images/shensuo1.jpg) no-repeat left top; cursor:pointer; line-height:0; font-size:0;overflow:hidden;}
#staffInfo .expendInfo .biao h3 img { float:left; margin-top:2px; margin-right:8px; _display:inline;}

#staffInfo .baseInfo .info { padding-bottom:31px; background:#ffffff; text-align:left; border:1px solid #e1be9f; border-top:none; overflow:hidden; zoom:1;}
#staffInfo .baseInfo .info .info_left { padding-left:4px; width:260px; overflow:hidden; zoom:1;float: left;}
#staffInfo .baseInfo .info .info_list { padding-left:20px; width:1000px; overflow:hidden; zoom:1;floadt: left;}


#staffInfo .expendInfo .info { padding-bottom:31px;  background:#ffffff; border:1px solid #e1be9f; border-top:none; overflow:hidden; zoom:1;}
#staffInfo .expendInfo .info .info_list { width:684px; overflow:hidden; zoom:1;}


/*2013-3-25*/
.zs_list { overflow:hidden; zoom:1; padding:0px 13px 15px 13px;}
.list_table { width:100%; border-collapse:collapse; border:1px solid #e1be9f;}
.list_table th { padding:1px 1px 1px 0px; height:29px; background:#fff9eb;}
.list_table th.first { padding-left:1px;}
.list_table th span { display:block; height:29px; line-height:28px; font-family:"宋体"; font-size:12px; color:#a87122; background:url(../images/list_table_th.gif) repeat-x left top;}
.list_table td { height:26px; border:1px solid #e1be9f; text-align:center; font-family:"宋体"; font-size:12px; color:#a87122;}
.list_table td a { display:block; height:18px; width:44px; background:url(../images/bianji_a.gif) no-repeat left top; text-decoration:none; text-align:center; line-height:18px; font-family:"宋体"; font-size:12px; color:#a87122; cursor:pointer;}

#maintainInfo { margin:15px 13px; border:1px solid #e1be9f; border-bottom:none;}
.biaoti { padding-left:14px; padding-right:25px; background:#fffcf4; height:31px; border-bottom:1px solid #e1be9f;}
.biaoti h3 { float:left; height:31px; line-height:30px; padding-left:28px;url(../images/biaoti_h3.gif) no-repeat left 3px; font-family:"宋体"; font-size:16px; color:#a87122;}

.shensuo { float:right; display:block; height:31px; width:11px; background:url(../images/shensuo.gif) no-repeat left 7px; cursor:pointer;}
.maintain_con { text-align:center; padding-top:30px; padding-left:20px; padding-bottom:45px; border-bottom:1px solid #e1be9f; display:none;}
.maintain_con h3 { height:20px; line-height:20px; text-align:left; font-family:"宋体"; font-size:16px; color:#a87122;}
.maintain_con p { height:30px; padding-top:24px;}
#addNewStaff { height:30px; width:92px; border:none; background:url(../images/staffBefore.gif) no-repeat center top; cursor:pointer; font-family:"宋体"; font-size:12px; color:#aa6708;}
#StaffExit { height:30px; width:92px; border:none; background:url(../images/staffBefore.gif) no-repeat center top; margin-left:9px; cursor:pointer; font-family:"宋体"; font-size:12px; color:#aa6708; color:#aa6708;}
.queryAll { height:30px; width:92px; border:none; background:url(../images/staffBefore.gif) no-repeat center top; margin-left:8px; cursor:pointer; font-family:"宋体"; font-size:12px; color:#aa6708; color:#aa6708;}
#staffInfo #maintainInfo #addNewStaff.current,#StaffExit.current { background:url(../images/staffAfter.gif) no-repeat center top;}
.current  { height:30px; width:92px; border:none; background:url(../images/staffAfter.gif) no-repeat center top; margin-left:8px; cursor:pointer; font-family:"宋体"; font-size:12px; color:#aa6708;}
