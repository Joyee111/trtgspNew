@charset "utf-8";
/* CSS Document */

body,div,ul,li,dl,dt,dd,h3,h4,p,a,input,table,ul { padding:0; margin:0 auto;}



#staffInfo { padding-bottom:80px; overflow:hidden; zoom:1;}
#staffInfo #title { height:28px; background:#ffffff; border-bottom:1px solid #edd1b1;}
#staffInfo #title h4 { height:28px; height:28px; line-height:28px; font-family:"宋体"; font-size:12px; color:#b48849; padding-left:37px; background:url(../images/titleTu.gif) no-repeat 20px center; font-weight:normal;}
#staffInfo #title h4 a { text-decoration:none; color:#b48849;}

#staffInfo #title h4 a:hover { color:#F90;}


