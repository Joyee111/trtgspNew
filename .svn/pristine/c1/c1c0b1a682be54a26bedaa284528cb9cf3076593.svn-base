
#
# Version information
#
tilapia.build.version=@VERSION@
tilapia.build.time=@BUILD_TIME@


# All i18n files must be available in the WEB-INF\classes.
i18n.files=tilapia-i18n

# Rules for configuring feature panels
feature.panels.config=/WEB-INF/conf/features/panels/generic/config.xml

# Recordset action groups
objectinspector.recordactiongroup=download
objectinspector.recordactiongroup.download=ERDAS.services.BabelService.SendRecordToCZSAction
objectinspector.recordactiongroup.download=ERDAS.services.BabelService.DownloadOriginalFileAction
objectinspector.recordactiongroup.download=ERDAS.services.BabelService.DownloadShoeboxRecordAction
objectinspector.recordactiongroup.download=ERDAS.services.BabelService.DownloadKMLRecordAction
objectinspector.recordactiongroup=default
objectinspector.recordactiongroup.default=ERDAS.ui.ObjectInspector.AddAllToMapRecordAction
objectinspector.recordactiongroup.default=ERDAS.ui.ObjectInspector.AddAllAsOneToMapRecordAction


# ============== Includes ==============
# If these files are available in the classpath (such as
# WEB-INF\classes, they will be added to this file
tryimport=apollo-client.properties
tryimport=custom.properties


# ===========================
# XSL for presenting metadata
metadata.stylesheet.iso19139=/metadata/xslt/combined.xslt



# ============ Various configuration properties ============
# The default date offset to be used between start date, and end date, 
# in days
# editors.timestampintervaleditor.dateoffset=1

# The key to connect to Microsoft Bing Maps
#bingmaps.key=

# The URL to the terms of use for Microsoft Bing Maps
#bingmaps.termsofuse.url=

# The key to connect to Google Maps
#googlemaps.key=


# The limit in the number of features that are returned
# when performing a GetFeature operation (in other words: when 
# using the "get layer info" tool on WFS layers
# layerinfohandlers.wfshandler.maxfeatures = 100


# Uncomment this to enable the "custom geometry" option 
# for the clip-zip-ship panel geometries
#clipzipshippanel2.geometry.custombroker=true


